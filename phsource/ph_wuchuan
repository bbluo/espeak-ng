// Wuchuan dialect phonemes
// 继承base1基础音素表
phonemetable wuchuan base1

// Wuchuan-specific tones (10 tones)
phoneme 1    // 阴平 755 (高平)
  stress
  Tone(55, 55, envelope/p_level, NULL)
endphoneme

phoneme 2    // 阴上 A24 (高降)
  stress
  Tone(42, 24, envelope/p_fall, NULL)
endphoneme

phoneme 3    // 阴去 J11 (低平)
  stress
  Tone(11, 11, envelope/p_level, NULL)
endphoneme

phoneme 4    // 上阴入 14 (高短)
  stress
  Tone(14, 14, envelope/p_level, NULL)
endphoneme

phoneme 5    // 下阴入 J1 (低短)
  stress
  Tone(11, 11, envelope/p_level, NULL)
endphoneme

phoneme 6    // 阳平 144 (中平)
  stress
  Tone(44, 44, envelope/p_level, NULL)
endphoneme

phoneme 7    // 阳上 H33 (中平)
  stress
  Tone(33, 33, envelope/p_level, NULL)
endphoneme

phoneme 8    // 阳去 122 (中降)
  stress
  Tone(22, 12, envelope/p_fall, NULL)
endphoneme

phoneme 9    // 上阳入 13 (中短)
  stress
  Tone(13, 13, envelope/p_level, NULL)
endphoneme

phoneme 10   // 下阳入 12 (低短)
  stress
  Tone(12, 12, envelope/p_level, NULL)
endphoneme

// 吴川方言特有的浊化声母
phoneme b    // 古帮母浊化为[b]
  vcd blb stp
  lengthmod 6
  Vowelin f1=2  f2=1000 -50 -100  f3=-200 80
  Vowelout f1=2  f2=1000 -500 -300  f3=-300 80 brk
  WAV(b/b)
endphoneme

phoneme d    // 古端母浊化为[d]
  vcd alv stp
  lengthmod 5
  Vowelin f1=1  f2=1700 -300 300  f3=-100 80
  Vowelout f1=2 f2=1700 -300 300  f3=-100 80 brk
  WAV(d/d)
endphoneme

// 吴川方言特有的元音音素
phoneme â    // [ɐ] 央低元音（带声调符号）
  vwl starttype #a endtype #a
  length 160
  FMT(vowel/aa)
endphoneme

phoneme ê    // [e] 中前元音（带声调符号）
  vwl starttype #e endtype #e
  length 160
  FMT(vowel/e)
endphoneme

phoneme í    // [i] 高前元音（带声调符号）
  vwl starttype #i endtype #i
  length 160
  FMT(vowel/i)
endphoneme

phoneme α    // [a] 低央元音（希腊字母）
  vwl starttype #a endtype #a
  length 160
  FMT(vowel/a)
endphoneme

phoneme è    // [e] 中前元音（重音符号）
  vwl starttype #e endtype #e
  length 160
  FMT(vowel/e)
endphoneme

phoneme ú    // [u] 高后元音（带声调符号）
  vwl starttype #u endtype #u
  length 160
  FMT(vowel/u)
endphoneme

phoneme y    // [y] 圆唇前高元音
  vwl starttype #i endtype #i
  length 160
  FMT(vowel/y)
endphoneme

// 吴川方言特有的辅音音素变体
phoneme A    // [ɐ] 央低元音
  vwl starttype #a endtype #a
  length 160
  FMT(vowel/aa)
endphoneme

phoneme E    // [ɛ] 半开前元音
  vwl starttype #e endtype #e
  length 160
  FMT(vowel/e_mid)
endphoneme

phoneme O    // [ɔ] 半开后元音
  vwl starttype #o endtype #o
  length 160
  FMT(vowel/oo)
endphoneme

phoneme I    // [ɪ] 近高前元音
  vwl starttype #i endtype #i
  length 140
  FMT(vowel/i_2)
endphoneme

phoneme U    // [ʊ] 近高后元音
  vwl starttype #u endtype #u
  length 140
  FMT(vowel/u_2)
endphoneme

// 为wuchuan_rules添加缺失的音素

// 声调音素 2-9 和 0 (映射到10)
phoneme 2    // 阴上
  stress
  Tone(42, 24, envelope/p_fall, NULL)
endphoneme

phoneme 3    // 阴去
  stress
  Tone(11, 11, envelope/p_level, NULL)
endphoneme

phoneme 4    // 阳平
  stress
  Tone(21, 21, envelope/p_fall, NULL)
endphoneme

phoneme 5    // 阳上
  stress
  Tone(23, 23, envelope/p_rise, NULL)
endphoneme

phoneme 6    // 阳去
  stress
  Tone(22, 22, envelope/p_level, NULL)
endphoneme

phoneme 7    // 阴入
  stress
  Tone(5, 5, envelope/p_level, NULL)
endphoneme

phoneme 8    // 中入
  stress
  Tone(3, 3, envelope/p_level, NULL)
endphoneme

phoneme 9    // 阳入
  stress
  Tone(2, 2, envelope/p_level, NULL)
endphoneme

phoneme 0    // 高入 (映射到声调10)
  stress
  Tone(12, 12, envelope/p_level, NULL)
endphoneme

// 复合音素符号
phoneme I    // 用于复元音中的 [i] 成分
  vwl starttype #i endtype #i
  length 120
  FMT(vowel/i)
endphoneme

phoneme U    // 用于复元音中的 [u] 成分  
  vwl starttype #u endtype #u
  length 120
  FMT(vowel/u)
endphoneme

phoneme A    // 用于 â 音的映射
  vwl starttype #a endtype #a
  length 150
  FMT(vowel/a)
endphoneme

phoneme E    // 用于 e/ê 音的映射
  vwl starttype #e endtype #e
  length 140
  FMT(vowel/e)
endphoneme

phoneme O    // 用于 o 音的映射
  vwl starttype #o endtype #o
  length 140
  FMT(vowel/o)
endphoneme